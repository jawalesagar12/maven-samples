pipeline {
		agent any
		stages {
				stage ('Build'){
					steps {
						echo "Creating Build"
					}
				}
				stage ('Test'){
					steps {
						echo "Testing Build"
					}
				
				}
				stage ('Deploy'){
				steps {
						echo "Deploying Package"
					}
				}
				stage ('Monitor'){
				steps {
						echo "Monitoring Application"
					}
				}
		}
		}